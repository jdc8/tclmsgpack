package require tcltest
package require msgpack

tcltest::test msgpack-unpacker-1 {msgpack unpack} -body {
    msgpack unpack
} -returnCodes {error} -result {wrong # args: should be "msgpack unpack data"}

tcltest::test msgpack-unpacker-1 {msgpack unpack} -body {
    msgpack unpack \1\3
} -returnCodes {ok} -result {{positive_integer 1} {positive_integer 3}}

tcltest::test msgpack-unpacker-1 {msgpack unpack} -setup {
    msgpack packer p
    p pack map 3
    p pack int 1
    p pack int 2
    p pack int 3
    p pack int 4
    p pack int 5
    p pack int 6
} -body {
    msgpack unpack [p data]
} -cleanup {
    p destroy
} -returnCodes {ok} -result {{map {{positive_integer 1} {positive_integer 2} {positive_integer 3} {positive_integer 4} {positive_integer 5} {positive_integer 6}}}}

# cleanup
::tcltest::cleanupTests
return
